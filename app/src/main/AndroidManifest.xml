<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.pakaranjing">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon41apl"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon41apl"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,UnusedAttribute">
        <activity android:name="com.pakaranjing.MainDokterActivity"></activity>
        <activity
            android:name="com.pakaranjing.AboutActivity"
            android:parentActivityName="com.pakaranjing.MainActivity" />
        <activity
            android:name="com.pakaranjing.BantuanDokterActivity"
            android:parentActivityName="com.pakaranjing.MainActivity" />
        <activity
            android:name="com.pakaranjing.BantuanUserActivity"
            android:parentActivityName="com.pakaranjing.MainActivity" />
        <activity
            android:name="com.pakaranjing.Activity.RiwayatActivity"
            android:parentActivityName="com.pakaranjing.MainActivity" />
        <activity
            android:name="com.pakaranjing.Activity.PenyakitDetailActivity"
            android:parentActivityName="com.pakaranjing.Activity.PenyakitActivity"
            android:theme="@style/AppThemeNoBarNoLight"/>
        <activity
            android:name="com.pakaranjing.Activity.PenyakitActivity"
            android:parentActivityName="com.pakaranjing.MainActivity" />
        <activity
            android:name="com.pakaranjing.Activity.DiagnosaHasilActivity"
            android:parentActivityName="com.pakaranjing.Activity.DiagnosaActivity"
            android:theme="@style/AppThemeNoBarNoLight"/>

        <activity
            android:name="com.pakaranjing.Activity.DiagnosaActivity"
            android:parentActivityName="com.pakaranjing.MainActivity"
            android:theme="@style/AppThemeNoBarNoLight"/>

        <activity android:name="com.pakaranjing.Activity.RegisterActivity" />
        <activity android:name="com.pakaranjing.Activity.LoginActivity" />
        <activity android:name="com.pakaranjing.MainActivity" />
        <activity
            android:name="com.pakaranjing.SplashActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>